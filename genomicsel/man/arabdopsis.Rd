\examples{
data(arabdopsis)
str(arabdopsis)

pheno <- arabdopsis$pheno
y.name <- arabdopsis$pheno[,1]
pheno <- as.matrix(pheno[,3], ncol=1)
rownames(pheno) <- y.name
colnames(pheno) <- "1_LD"
geno <- t(arabdopsis$geno)

arabTT <- creatGPO (Ymat=pheno, Mmat=geno, Xmat=NULL,Kmat=NULL, map = NULL,  minMAF=0, minMISS = 0, thinINT = NULL, pRandSample = NULL, seed=NULL, 
expected.geno = -1:1)


arabTT1 <- doPreProcess (arabTT, impute.M = "meanImpute", transform.Y=NULL,  transform.X = NULL)

lst <- doDataPartition(arabTT, times = 1, partitions=2, p = c(0.8), seed = c(45))

data_tr <- sepPartition (lst, rep=1, part=1)
test_data <- sepPartition (lst, rep=1, part=2)

outTT <- fit.model(gpobj=data_tr, module = "rrBLUP", method = "rrBLUP", argument_list=NULL)
predtsTT <- predict(outTT, newM=test_data$Mmat, newX=NULL)
cor(predtsTT,test_data$Ymat)
plot(predtsTT,test_data$Ymat)

# rrBLUP
trainTT <- doTrain(data_tr, module = "rrBLUP", method = "rrBLUP",  validation = "cv", k = 10, times =1, seed=NULL)

plot(trainTT)

}
